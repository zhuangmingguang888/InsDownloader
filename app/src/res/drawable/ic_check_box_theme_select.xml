<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape
            android:shape="oval"
            android:useLevel="true">
            <stroke
                android:width="1.5dp"
                android:color="@color/text_guide_method_color" />
        </shape>
    </item>


    <item
        android:bottom="5.5dp"
        android:left="5.5dp"
        android:right="5.5dp"
        android:top="5.5dp">

        <shape
            android:shape="oval"
            android:useLevel="true">

            <solid android:color="@color/text_guide_method_color" />
        </shape>
    </item>



</layer-list>